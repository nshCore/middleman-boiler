{
  "name": "some-cool-site.tld",
  "version": "0.0.1",
  "scripts": {
    "build:assets": "NODE_ENV=production webpack -p --config webpack.prod.config.js",
    "start:assets": "NODE_ENV=development webpack --watch --config webpack.dev.config.js",
    "middleman:install": "bundle install",
    "middleman:build": "bundle exec middleman build",
    "middleman:start": "bundle exec middleman server",
    "rake:test": "rake test"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "font-awesome": "^4.7.0",
    "normalize-scss": "^7.0.0"
  },
  "devDependencies": {
    "ajv": "^6.10.0",
    "ajv-keywords": "^3.4.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bulma": "^0.7.4",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^1.1.6",
    "modernizr-webpack-plugin": "^1.0.6",
    "node-gyp": "^4.0.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "phantomjs-prebuilt": "^2.1.13",
    "postcss-loader": "^2.0.9",
    "progress-bar-webpack-plugin": "^1.10.0",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "resolve-url-loader": "^2.2.1",
    "sass": "^1.20.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-merge": "^4.1.1"
  }
}
